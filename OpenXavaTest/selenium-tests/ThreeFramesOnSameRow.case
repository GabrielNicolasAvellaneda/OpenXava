<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head profile="http://selenium-ide.openqa.org/profiles/test-case">
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<link rel="selenium.base" href="http://localhost:8080/" />
<title>ThreeFramesOnSameRow</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">ThreeFramesOnSameRow</td></tr>
</thead><tbody>
<tr>
	<td>open</td>
	<td>/OpenXavaTest/modules/CustomerThreeFramesOnSameRow</td>
	<td></td>
</tr>
<tr>
	<td>waitForTitle</td>
	<td>OpenXavaTest - Customer three frames on same row</td>
	<td></td>
</tr>
<tr>
	<td>waitForElementPresent</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__CRUD___new</td>
	<td></td>
</tr>
<tr>
	<td>click</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__CRUD___new</td>
	<td></td>
</tr>
<!--To make sure everything is painted-->
<tr>
	<td>pause</td>
	<td>1000</td>
	<td></td>
</tr>
<tr>
	<td>storeElementPositionTop</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_addresscontent</td>
	<td>addressTop</td>
</tr>
<tr>
	<td>storeElementPositionLeft</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_addresscontent</td>
	<td>addressLeft</td>
</tr>
<tr>
	<td>storeElementWidth</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_addresscontent</td>
	<td>addressWidth</td>
</tr>
<tr>
	<td>storeElementPositionTop</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_sellercontent</td>
	<td>sellerTop</td>
</tr>
<tr>
	<td>storeElementPositionLeft</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_sellercontent</td>
	<td>sellerLeft</td>
</tr>
<tr>
	<td>storeElementWidth</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_sellercontent</td>
	<td>sellerWidth</td>
</tr>
<tr>
	<td>storeElementPositionTop</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_alternateSellercontent</td>
	<td>alternateSellerTop</td>
</tr>
<tr>
	<td>storeElementPositionLeft</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_alternateSellercontent</td>
	<td>alternateSellerLeft</td>
</tr>
<tr>
	<td>storeElementWidth</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_alternateSellercontent</td>
	<td>alternateSellerWidth</td>
</tr>
<tr>
	<td>assertElementPositionTop</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_sellercontent</td>
	<td>${addressTop}</td>
</tr>
<tr>
	<td>assertElementPositionTop</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_alternateSellercontent</td>
	<td>${addressTop}</td>
</tr>
<tr>
	<td>assertElementPositionLeft</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_sellercontent</td>
	<td>javascript{storedVars['addressLeft'] + storedVars['addressWidth'] + 10}</td>
</tr>
<tr>
	<td>assertElementPositionLeft</td>
	<td>id=ox_OpenXavaTest_CustomerThreeFramesOnSameRow__frame_group_alternateSellercontent</td>
	<td>javascript{storedVars['addressLeft'] + storedVars['addressWidth'] + storedVars['sellerWidth'] + 20}</td>
</tr>
</tbody></table>
</body>
</html>
