<!DOCTYPE hibernate-configuration PUBLIC 	"-//Hibernate/Hibernate Configuration DTD//EN"	"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">	
<hibernate-configuration>	<session-factory>		<property name="hibernate.connection.datasource">java:comp/env/jdbc/OpenXavaTestDS</property>		<property name="hibernate.dialect">org.hibernate.dialect.HSQLDialect</property>	
		<property name="hibernate.show_sql">true</property>	
		<property name="hibernate.jdbc.use_get_generated_keys">false</property>	</session-factory></hibernate-configuration>