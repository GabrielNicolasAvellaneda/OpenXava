<!-- Sessions Beans de Xava -->
		<session id="MapFacade">            
			<ejb-name>MapFacade</ejb-name>            
			<home>org.openxava.model.impl.MapFacadeHome</home>            
			<remote>org.openxava.model.impl.MapFacadeRemote</remote>            
			<ejb-class>org.openxava.model.impl.MapFacadeBean</ejb-class>            
			<session-type>Stateless</session-type>            
			<transaction-type>Container</transaction-type>         
		</session>
		<session id="Server">            
			<ejb-name>Server</ejb-name>            
			<home>org.openxava.util.impl.ServerHome</home>            
			<remote>org.openxava.util.impl.ServerRemote</remote>            
			<ejb-class>org.openxava.util.impl.ServerBean</ejb-class>            
			<session-type>Stateless</session-type>            
			<transaction-type>Container</transaction-type>         
			<env-entry>
				<env-entry-name>DATA_SOURCE</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>jdbc/DataSource</env-entry-value>
			</env-entry>
			<resource-ref id="Server_datasource">
				<res-ref-name>jdbc/DataSource</res-ref-name>
				<res-type>javax.sql.DataSource</res-type>
				<res-auth>Container</res-auth>
			</resource-ref>			
		</session>		
		<session id="EntityTab">
			<ejb-name>EntityTab</ejb-name>
			<home>org.openxava.tab.impl.EntityTabHome</home>
			<remote>org.openxava.tab.impl.EntityTabRemote</remote>
			<ejb-class>org.openxava.tab.impl.EntityTabBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<env-entry>
				<env-entry-name>DATA_SOURCE</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>jdbc/DataSource</env-entry-value>
			</env-entry>
			<resource-ref id="EntityTab_datasource">
				<res-ref-name>jdbc/DataSource</res-ref-name>
				<res-type>javax.sql.DataSource</res-type>
				<res-auth>Container</res-auth>
			</resource-ref>
		</session>      
