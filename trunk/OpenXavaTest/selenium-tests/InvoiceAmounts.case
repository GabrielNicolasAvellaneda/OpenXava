<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head profile="http://selenium-ide.openqa.org/profiles/test-case">
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<link rel="selenium.base" href="http://localhost:8080/" />
<title>InvoiceAmounts</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">InvoiceAmounts</td></tr>
</thead><tbody>
<!--Liferay Aware-->
<tr>
	<td>gotoIf</td>
	<td>&quot;${liferay}&quot;!=&quot;true&quot;</td>
	<td>afterOpenInvoiceAmounts</td>
</tr>
<tr>
	<td>lrToPage</td>
	<td>/web/guest/</td>
	<td>InvoiceAmounts</td>
</tr>
<tr>
	<td>goto</td>
	<td>afterLRPageInvoiceAmounts</td>
	<td></td>
</tr>
<tr>
	<td>label</td>
	<td>afterOpenInvoiceAmounts</td>
	<td></td>
</tr>
<!--Non Liferay-->
<tr>
	<td>open</td>
	<td>/OpenXavaTest/m/InvoiceAmounts</td>
	<td></td>
</tr>
<tr>
	<td>waitForTitle</td>
	<td>OpenXavaTest - Invoice amounts</td>
	<td></td>
</tr>
<tr>
	<td>label</td>
	<td>afterLRPageInvoiceAmounts</td>
	<td></td>
</tr>
<tr>
	<td>click</td>
	<td>id=ox_OpenXavaTest_InvoiceAmounts__CRUD___new</td>
	<td></td>
</tr>
<tr>
	<td>waitForValue</td>
	<td>id=ox_OpenXavaTest_InvoiceAmounts__number</td>
	<td></td>
</tr>
<!--Store Columns Position-->
<tr>
	<td>pause</td>
	<td>2000</td>
	<td></td>
</tr>
<tr>
	<td>storeElementPositionLeft</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__customerDiscount</td>
	<td>column1</td>
</tr>
<tr>
	<td>storeElementPositionLeft</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__customerTypeDiscount</td>
	<td>column2</td>
</tr>
<tr>
	<td>storeElementPositionLeft</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__yearDiscount</td>
	<td>column3</td>
</tr>
<!--Store Row positions-->
<tr>
	<td>storeElementPositionTop</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__customerDiscount</td>
	<td>row1</td>
</tr>
<tr>
	<td>storeElementPositionTop</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__amountsSum</td>
	<td>row2</td>
</tr>
<!--Test positions-->
<!--  Columns-->
<tr>
	<td>assertElementPositionLeft</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__amountsSum</td>
	<td>${column1}</td>
</tr>
<tr>
	<td>assertElementPositionLeft</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__vatPercentage</td>
	<td>${column2}</td>
</tr>
<tr>
	<td>assertElementPositionLeft</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__vat</td>
	<td>${column3}</td>
</tr>
<!--  Rows-->
<tr>
	<td>assertElementPositionTop</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__customerTypeDiscount</td>
	<td>${row1}</td>
</tr>
<tr>
	<td>assertElementPositionTop</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__yearDiscount</td>
	<td>${row1}</td>
</tr>
<tr>
	<td>assertElementPositionTop</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__vatPercentage</td>
	<td>${row2}</td>
</tr>
<tr>
	<td>assertElementPositionTop</td>
	<td>name=ox_OpenXavaTest_InvoiceAmounts__vat</td>
	<td>${row2}</td>
</tr>
</tbody></table>
</body>
</html>
